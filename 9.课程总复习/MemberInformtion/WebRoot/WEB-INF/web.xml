<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" 
	xmlns="http://java.sun.com/xml/ns/javaee" 
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
	http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
  <display-name></display-name>
  <servlet>
    <servlet-name>FindAllMembersinfoServlet</servlet-name>
    <servlet-class>com.bdqn.memberinfomtion.servlet.FindAllMembersinfoServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>GetMIdFindMembersinfoServlet</servlet-name>
    <servlet-class>com.bdqn.memberinfomtion.servlet.GetMIdFindMembersinfoServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>UpdateMembersinfoServlet</servlet-name>
    <servlet-class>com.bdqn.memberinfomtion.servlet.UpdateMembersinfoServlet</servlet-class>
  </servlet>



  <servlet-mapping>
    <servlet-name>FindAllMembersinfoServlet</servlet-name>
    <url-pattern>/FindAllMembersinfoServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>GetMIdFindMembersinfoServlet</servlet-name>
    <url-pattern>/GetMIdFindMembersinfoServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>UpdateMembersinfoServlet</servlet-name>
    <url-pattern>/UpdateMembersinfoServlet</url-pattern>
  </servlet-mapping>	
  <welcome-file-list>
    <welcome-file>FindAllMembersinfoServlet</welcome-file>
  </welcome-file-list>
</web-app>
